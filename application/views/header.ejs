<nav class="navbar navbar-inverse navbar-fixed-top" id="sidebar-wrapper" role="navigation" ng-controller="HeaderController" ng-init="init('<%- dashboard.id %>')">
  <ul class="nav sidebar-nav">
    <li class="sidebar-brand">
      <a href="/">
         Dashboards
      </a>
    </li>
    <li>
      <a href="#" ng-click="openMarketplace()">
        <i class="glyphicon glyphicon-th"></i> Marketplace
      </a>
    </li>
    <li role="presentation" class="divider"></li>
    <li ng-repeat="dashboard in dashboards" ng-class="(currentDashboard.id == dashboard.id) ? 'selected' : ''">
