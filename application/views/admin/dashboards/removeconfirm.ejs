<% include ../header.ejs %>

<div id="admin-page">
	<div class="container">
		<h1>Remove <%= result.name %></h1>
		<hr />
		<div class="alert alert-warning">
			<p><strong>Are you sure you want to remove this dashboard?</strong></p>
			<p>This action cannot be undone</p>
		</div>
		<form action="/admin/dashboards/<%= result.id %>/remove" method="POST">
			<div class="form-group">
				<button class="btn btn-danger" type="submit">Remove</button>
				<a href="/admin/applications" class="btn btn-info">Cancel</a>
			</div>
		</form>
	</div>
</div>